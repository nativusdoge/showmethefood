<ul class="media-grid">
  <% @cookBook.recipes.each do |recipe| %>
  <li>
    <%= link_to recipe.link_url do %>
      <img class="thumbnail" src=<%= "#{recipe.image_url}" %> alt=<%= "\"#{recipe.description}\"" %> width="290" height="207" >
      <h5><%= recipe.description %></h5>
    <% end %>
  </li>
  <% end %>
</ul>